# CastReach Environment Variables
# Copy this file to .env.local and fill in your values

# App Configuration
VITE_APP_NAME=CastReach
VITE_APP_VERSION=0.3.0
VITE_APP_URL=http://localhost:5173

# API Configuration (for future backend integration)
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Supabase Configuration (when ready)
# VITE_SUPABASE_URL=your_supabase_url
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration (when ready)
# VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Google Calendar API (when ready)
# VITE_GOOGLE_CLIENT_ID=your_google_client_id
# VITE_GOOGLE_API_KEY=your_google_api_key

# Cloudinary Configuration (for file uploads, when ready)
# VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
# VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Analytics (when ready)
# VITE_GA_TRACKING_ID=your_google_analytics_id

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PAYMENTS=false
VITE_ENABLE_REAL_TIME=false

# Development
VITE_DEBUG_MODE=true
VITE_MOCK_API=true
